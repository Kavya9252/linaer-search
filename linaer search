#include <stdio.h>
int main()
{
   int a[10],i=0,n,searchdata;
   printf("Enter the size of the array");
   scanf("%d",&n);
   printf("Enter the elements of array");
   for(i=0;i<n;i++)  
   scanf("%d",&a[i]);
   printf("Enter the search data");
   scanf("%d",&searchdata);
  for(i=0;i<n;i++)
  
      if(a[i]==searchdata)
      {
          printf("Element is found at the index %d",i+1);
          break;
      }
      else 
      printf("Element is not found\n");
}
